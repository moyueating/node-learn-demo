<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>


    <div class="box">
        <% pics.forEach(function(pic){ %>
            <div class="pic">
                <h3><%= pic.name%></h3>
                <img src="<%= pic.path%>" alt="">
            </div>
        <% }) %>
    </div>

    <div class="add">新增</div>

    <script src="/js/jq.js"></script>
    <script src="/js/index.js"></script>
  </body>
</html>
