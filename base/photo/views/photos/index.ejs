<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>


    <div class="box">
        <% photos.forEach(function(photo){ %>
            <div class="photo">
                <h3><%= photo.name%></h3>
                <img src="<%= photo.path%>" alt="">
            </div>
        <% }) %>
    </div>

    
  </body>
</html>
